{
    "name": "api",
    "version": "1.0.0",
    "description": "REST API scaffolding",
    "license": "MIT",
    "author": "Tiberiu Feredean",
    "repository": {
        "type": "git",
        "url": "https://github.com/feredean/node-api-starter"
    },
    "scripts": {
        "start": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"tsc -w\" \"nodemon dist/server.js\"",
        "test": "jest --coverage --verbose",
        "test:watch": "npm run test -- --watchAll",
        "test:debugger": "node --inspect-brk node_modules/.bin/jest --runInBand --testTimeout=300000",
        "test:debugger:watch": "npm run test:debugger -- --watchAll",
        "build": "npm run build-ts && npm run lint",
        "build-ts": "tsc",
        "lint": "tsc --noEmit && eslint \"*/**/*.{js,ts}\" --quiet --fix",
        "check-deps": "npm audit && ncu"
    },
    "dependencies": {
        "@types/express-handlebars": "^3.1.0",
        "@types/swagger-ui-express": "^4.1.2",
        "aws-sdk": "2.668.0",
        "bcrypt": "5.0.0",
        "body-parser": "1.19.0",
        "compression": "1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "2.8.5",
        "csv-stringify": "^5.6.1",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "fbgraph": "1.4.4",
        "handlebars": "^4.7.6",
        "jsonwebtoken": "8.5.1",
        "lodash": "4.17.19",
        "mongoose": "5.9.11",
        "morgan": "1.10.0",
        "multer": "1.4.2",
        "multer-s3": "^2.9.0",
        "nodemailer": "6.4.6",
        "nodemailer-express-handlebars": "^4.0.0",
        "nodemailer-handlebars": "^1.0.1",
        "passport": "0.4.1",
        "passport-facebook": "3.0.0",
        "passport-google-oauth": "^2.0.0",
        "passport-linkedin": "^1.0.0",
        "passport-linkedin-oauth2": "^2.0.0",
        "passport-local": "1.0.0",
        "rate-limiter-flexible": "^2.2.1",
        "stripe": "^8.119.0",
        "swagger-jsdoc": "^4.2.0",
        "swagger-ui-express": "^4.1.4",
        "uuid": "3.4.0",
        "validator": "13.1.1",
        "winston": "3.2.1",
        "ws": "^7.4.2"
    },
    "devDependencies": {
        "@types/bcrypt": "3.0.0",
        "@types/compression": "1.7.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "2.8.7",
        "@types/express": "4.17.7",
        "@types/express-handlebars": "3.1.0",
        "@types/handlebars": "^4.1.0",
        "@types/jest": "26.0.12",
        "@types/jsonwebtoken": "8.5.0",
        "@types/lodash": "4.14.161",
        "@types/mongoose": "5.7.14",
        "@types/morgan": "1.9.1",
        "@types/multer": "1.4.4",
        "@types/multer-s3": "^2.7.9",
        "@types/nodemailer": "6.4.0",
        "@types/passport": "^1.0.4",
        "@types/passport-facebook": "2.1.10",
        "@types/passport-google-oauth": "^1.0.41",
        "@types/passport-linkedin-oauth2": "^1.5.1",
        "@types/passport-local": "1.0.33",
        "@types/supertest": "2.0.10",
        "@types/swagger-jsdoc": "^3.0.2",
        "@types/uuid": "7.0.4",
        "@types/validator": "13.0.0",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "2.30.0",
        "@typescript-eslint/parser": "2.30.0",
        "concurrently": "5.2.0",
        "eslint": "7.3.1",
        "jest": "26.0.1",
        "nodemon": "2.0.3",
        "npm-check-updates": "7.0.1",
        "supertest": "4.0.2",
        "ts-jest": "26.1.0",
        "typescript": "3.8.3"
    }
}
